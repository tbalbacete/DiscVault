{
  "name": "discvault",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "dotenv -e .env.local -e .env node ./envjs-generator.mjs ./public/env.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext .ts --ext .tsx --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "preview": "vite preview",
    "generate": "rimraf ./src/api && openapi-generator-cli generate -i openapi.json -o ./src/api --generator-name typescript-fetch --additional-properties=typescriptThreePlus=true,supportsES6=true --type-mappings Created=string,Modified=string,Day=string",
    "prepare": "cd .. && husky DiscVaultFrontend/.husky",
    "format": "prettier --ignore-unknown \"src/**/*.*\"",
    "format:check": "yarn run format --check",
    "storybook": "storybook dev -p 6006 --config-dir ./src/packages/components/.storybook",
    "plop": "plop component --plopfile \".plop/plop.js\"",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@datadog/browser-rum": "^5.22.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/ibm-plex-mono": "^5.0.13",
    "@fontsource/ibm-plex-sans": "^5.0.20",
    "@mui/material": "^5.15.20",
    "@mui/x-date-pickers": "^7.11.0",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rollup/plugin-replace": "^5.0.7",
    "@storybook/addon-controls": "^8.1.10",
    "@storybook/addon-docs": "^8.1.10",
    "@storybook/addon-essentials": "^8.1.4",
    "@storybook/addon-interactions": "^8.1.4",
    "@storybook/addon-links": "^8.1.4",
    "@storybook/blocks": "^8.1.4",
    "@storybook/react": "^8.1.4",
    "@storybook/react-vite": "^8.1.4",
    "@storybook/test": "^8.1.4",
    "@tanstack/react-query": "^5.49.0",
    "@tanstack/react-query-devtools": "^5.49.2",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "dayjs": "^1.11.11",
    "dotenv-cli": "^7.4.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "formik": "2.2.9",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "jest-axe": "^9.0.0",
    "jotai": "^2.8.3",
    "jsdom": "^24.1.0",
    "material-ui-popup-state": "^5.1.2",
    "msw": "^2.3.0",
